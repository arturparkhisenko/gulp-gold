{
  "name": "gulp-gold",
  "description": "Custom Gulp pack",
  "version": "1.3.1",
  "main": "gulpfile.js",
  "jsnext:main": "gulpfile.js",
  "author": "Artur Parkhisenko <ikeagold@gmail.com> (https://arturparkhisenko.github.io/)",
  "homepage": "https://github.com/arturparkhisenko/gulp-gold/",
  "repository": "arturparkhisenko/gulp-gold",
  "bugs": {
    "url": "https://github.com/arturparkhisenko/gulp-gold/issues"
  },
  "keywords": [
    "html",
    "css",
    "js",
    "build",
    "tool",
    "asset",
    "babel",
    "es6",
    "es2015",
    "webpack",
    "postcss",
    "cssnext",
    "css4",
    "gulp"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.1",
    "npm": ">= 5.5.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              ">1%"
            ]
          },
          "modules": false,
          "loose": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "license": "MIT",
  "scripts": {
    "gulp": "./node_modules/.bin/gulp",
    "build": "cross-env NODE_ENV=production gulp",
    "start": "cross-env NODE_ENV=development gulp serve",
    "dev": "cross-env NODE_ENV=development gulp serve",
    "prod": "cross-env NODE_ENV=production gulp serve",
    "lintStyles": "gulp lintStyles",
    "lintScripts": "gulp lintScripts",
    "formatScripts": "prettier-eslint --write ./src/scripts/**/*.js",
    "np": "np --no-yarn"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "browser-sync": "^2.24.7",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.0",
    "del": "^3.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "fancy-log": "^1.3.2",
    "gulp": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-minify-inline": "^1.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-stylelint": "^7.0.0",
    "plugin-error": "^1.0.1",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-reporter": "^6.0.0",
    "postcss-url": "^8.0.0",
    "prettier-eslint-cli": "^4.7.1",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0"
  }
}
