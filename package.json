{
  "name": "gulp-gold",
  "description": "Custom Gulp pack",
  "version": "1.3.0",
  "main": "gulpfile.js",
  "jsnext:main": "gulpfile.js",
  "author": "Artur Parkhisenko <ikeagold@gmail.com> (https://arturparkhisenko.github.io/)",
  "homepage": "https://github.com/arturparkhisenko/gulp-gold/",
  "repository": "arturparkhisenko/gulp-gold",
  "bugs": {
    "url": "https://github.com/arturparkhisenko/gulp-gold/issues"
  },
  "keywords": [
    "html",
    "css",
    "js",
    "build",
    "tool",
    "asset",
    "babel",
    "es6",
    "es2015",
    "webpack",
    "postcss",
    "cssnext",
    "css4",
    "gulp"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.1",
    "npm": ">= 5.5.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              ">1%"
            ]
          },
          "modules": false,
          "loose": true
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "license": "MIT",
  "scripts": {
    "gulp": "./node_modules/.bin/gulp",
    "build": "cross-env NODE_ENV=production gulp",
    "start": "cross-env NODE_ENV=development gulp serve",
    "dev": "cross-env NODE_ENV=development gulp serve",
    "prod": "cross-env NODE_ENV=production gulp serve",
    "lintStyles": "gulp lintStyles",
    "lintScripts": "gulp lintScripts",
    "formatScripts": "prettier-eslint --write ./src/scripts/**/*.js",
    "np": "np --no-yarn"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.23.6",
    "cross-env": "^5.1.4",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.10.0",
    "fancy-log": "^1.3.2",
    "gulp": "^4.0.0",
    "gulp-eslint": "^4.0.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-minify-inline": "^1.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-stylelint": "^7.0.0",
    "plugin-error": "^1.0.1",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-preset-env": "^4.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.2",
    "prettier-eslint-cli": "^4.7.1",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14"
  }
}
